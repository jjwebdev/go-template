{
  "name": "react-mobx-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "stop": "pkill --signal SIGINT KBS",
    "webpack:hot": "NODE_ENV=hot node webpack-dev-server.js",
    "webpack:build:production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --config webpack.config.js --progress",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_PATH=./src mocha --recursive --compilers css:./test/css-null-compiler.js test/mocha-setup.js test/unit",
    "test:e2e": "./node_modules/.bin/wdio test/wdio.conf.js",
    "test:unit:watch": "NODE_PATH=./src mocha --recursive --compilers css:./test/css-null-compiler.js test/mocha-setup.js test/unit -w",
    "prepublish": "./node_modules/.bin/selenium-standalone install"
  },
  "repository": {
    "type": "git"
  },
  "author": "Eudis Duran",
  "license": "ISC",
